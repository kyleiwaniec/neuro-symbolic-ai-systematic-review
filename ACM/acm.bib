@article{10.1007/s10849-019-09283-6,
author = {Evert, Stefan and Heinrich, Philipp and Henselmann, Klaus and Rabenstein, Ulrich and Scherr, Elisabeth and Schmitt, Martin and Schr\"{o}der, Lutz},
title = {Combining Machine Learning and Semantic Features in the Classification of Corporate Disclosures},
year = {2019},
issue_date = {June      2019},
publisher = {Kluwer Academic Publishers},
address = {USA},
volume = {28},
number = {2},
issn = {0925-8531},
url = {https://doi.org/10.1007/s10849-019-09283-6},
doi = {10.1007/s10849-019-09283-6},
abstract = {We investigate an approach to improving statistical text classification by combining
machine learners with an ontology-based identification of domain-specific topic categories.
We apply this approach to ad hoc disclosures by public companies. This form of obligatory
publicity concerns all information that might affect the stock price; relevant topic
categories are governed by stringent regulations. Our goal is to classify disclosures
according to their effect on stock prices (negative, neutral, positive). In the study
reported here, we combine natural language parsing with a formal background ontology
to recognize disclosures concerning particular topics from a prescribed list. The
semantic analysis identifies some of these topics with reasonable accuracy. We then
demonstrate that machine learners benefit from the additional ontology-based information
when predicting the cumulative abnormal return attributed to the disclosure at hand.},
journal = {J. of Logic, Lang. and Inf.},
month = jun,
pages = {309–330},
numpages = {22},
keywords = {Ontology, NLP, Machine learning, Corporate disclosures}
}

@inproceedings{10.1109/FUZZ-IEEE.2019.8858791,
author = {Ahn, Seokjin and Couture, St\'{e}fan V. and Cuzzocrea, Alfredo and Dam, Kevin and Grasso, Giorgio M. and Leung, Carson K. and McCormick, Kaleigh L. and Wodi, Bryan H.},
title = {A Fuzzy Logic Based Machine Learning Tool for Supporting Big Data Business Analytics in Complex Artificial Intelligence Environments},
year = {2019},
publisher = {IEEE Press},
url = {https://doi.org/10.1109/FUZZ-IEEE.2019.8858791},
doi = {10.1109/FUZZ-IEEE.2019.8858791},
abstract = {Business analytics use techniques from data science, data mining, artificial intelligence
(especially, machine learning), mathematics and statistics to gain insights and understanding
on the performance of business processes. The gained insights and knowledge help driving
the business planning. As employees play important roles in the business process,
having a tool to classify and predict their wage levels is desirable. Such classification
and prediction enables the public or private sector to offer competitive wages for
recruiting and retaining employees. In this paper, we present a tool for classifying
and predicting wage levels. It incorporates fuzzy logic into a machine-learning tool
to support business analytics on big data. Evaluation results show the applicability
of our tool for classification and prediction of wages levels in the business world,
which in turn supports business analytics in complex artificial intelligence environments.},
booktitle = {2019 IEEE International Conference on Fuzzy Systems (FUZZ-IEEE)},
pages = {1–6},
numpages = {6},
location = {New Orleans, LA, USA}
}

@inproceedings{10.5555/3323234.3323272,
author = {Jeong, Eunji and Cho, Sungwoo and Yu, Gyeong-In and Jeong, Joo Seong and Shin, Dong-Jin and Chun, Byung-Gon},
